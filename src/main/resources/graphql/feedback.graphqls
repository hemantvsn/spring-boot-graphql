type Feedback {
    id: String
    feedbackText: String
    articleId: String
    createdByUserId: String
    createdOn: String
    lastUpdatedOn: String
}


extends type Query {
	getFeedBacksForArticle(pageNumber : Int!, pageSize : Int!, articleId: String!) : [Feedback]
}

extends type Mutation {
	createNewFeedback(feedbackText : String!, articleId : String!, createdByUser : String !)
	archiveFeedback(id : String!)
}